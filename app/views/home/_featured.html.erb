<div class="container_16">

  <div class="featured-content clearfix">
      <div class="homepage-header">Featured classes</div><br><br>
      
          <div class="classRow-homepage clearfix">
              
              <div class="classBlock-homepage">
                  <%= link_to (image_tag(@fav1.photo.url(:small), :size => "190x120")), @fav1 %>
                  <%= image_tag('v2/arrow_white_pointup.png', :class => 'class-arrow') %>

                <div class="class-description">
                  <div class="heading13-class"><%= link_to @fav1.title.truncate(45), @fav1 %></div>
                  <p><%= @fav1.teaser.truncate(60) unless @fav1.teaser.blank? %></p>
                </div><!-- /end .class-description -->
                <div class="class-note">
                  <div class="heading9-class">
                    <% if @fav1.max_seats.blank? %>
                      <%= link_to "Unlimited seats |", @fav1 %>
                    <% elsif @fav1.seats_left == 0 %>
                      <%= link_to "Full! Request again |", @fav1 %>
                    <% elsif %>
                      <%= "#{@fav1.seats_left} seats left |" %>
                    <% end %>
                    <%= "#{@fav1.city.try(:name)}, #{@fav1.city.try(:state)}" %>
                  </div>
                </div><!-- /end .class-note -->
              </div><!-- /end .classBlock -->

