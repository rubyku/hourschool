<div id="header">
  <div class="container_16">
    
    <!-- <a href="/p/dashboard" id="logo"> -->
	<%= link_to image_tag("/images/v2/Logo_Orange.png"), user_root_path, :id => "logo" %>
     
	
<div id="login">
	
        <ul id="login">
          <li id="login">Peer learning in Austin</li>
          <li id="login">
					<% if user_signed_in? %>
						<%#= link_to current_user.name, profile_path, :id => "menu"%>
						<%= current_user.name %>
						<img src=" /images/v2/Avatars_04_V1.png" class="profile-avatar">
							<span id="nav-dropdown">

					              <a href="/profile" id="menu">   

					                <img src="/images/v2/arrow_grey_profile.png" class="profile-arrow">
					              </a>
							</span>

							<span id="nav-dropdown-selected" style="display:none;">
				              <a href="/profile" id="menu">   
				                <%#= current_user.name%>
				                <img src="/images/v2/arrow_grey_profile.png" class="profile-arrow">
				              </a></span>
					<% else %>
						<%= link_to "sign in", new_user_session_path, :id => "menu"%>
					<% end %>
					
				</li>
	
		  <% if user_signed_in? %>
          <div id="dropdown-box" style="display:none;">
            <table id="dropdown-text">
              <tr id="dropdown-text"><td id="dropdown-text"><%= link_to "My profile", profile_path %></td></tr>
              <tr id="dropdown-text"><td id="dropdown-text"><%= link_to "Settings", edit_user_registration_path %></td></tr>
              <tr id="dropdown-text-last"><td id="dropdown-text"> <%= link_to "Logout", destroy_user_session_path %></td></tr>
            </table>
          </div>
		  <% end %>
        </ul>
    </div>

    <script type="text/javascript">
        $("#nav-dropdown").click(function() {
            $("#dropdown-box").show();
            $("#nav-dropdown").hide();
            $("#nav-dropdown-selected").show();
            return false;
         });
    	 $("#nav-dropdown-selected").click(function() {
             $("#dropdown-box").hide();
             $("#nav-dropdown").show();
             $("#nav-dropdown-selected").hide();
        });
    </script> 
    
  
    
   
    <script type="text/javascript">
        $("#nav-dropdown").click(function() {
            $("#dropdown-box").show();
            $("#nav-dropdown").hide();
            $("#nav-dropdown-selected").show();
            return false;
         });
    	 $("#nav-dropdown-selected").click(function() {
             $("#dropdown-box").hide();
             $("#nav-dropdown").show();
             $("#nav-dropdown-selected").hide();
             return false;
        });
    </script>
    
        <table id="menu">
