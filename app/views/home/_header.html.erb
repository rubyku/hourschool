                  <div id="menu18">Learn</div> 
                  <div id="menu9">something new</div>
                <!-- </a> -->
				<% end %>
              </td>
              <td id="community_menu_teach">
                <%= link_to teach_path, :id => "menu" do %>
                  <div id="menu9">Offer to</div>
                  <div id="menu18">teach</div>
                <% end %>
              </td>
            <td id="community_menu_suggest">
			<%= link_to new_csuggestion_path, :id => "menu" do %>
              
                <div id="menu18">Suggest</div>
                <div id="menu9">a class</div>
              <% end %>
            </td>
            <td id="community_menu_community">
              <%= link_to business_path, :id => "menu" do %>
                <div id="menu9">HourSchool for</div>
                <div id="menu18">Business</div>
              <% end %>
            </td>
            <td id="community_menu_faq">
              <%= link_to community_faq_path, :id => "menu" do %>
                <div id="menu_faq">F</div>
                <div id="menu_faq">A</div>
                <div id="menu_faq">Q</div>
              <% end %>
            </td>
          </tr>
        </table>


  </div><!-- /end .container_16 -->

</div><!-- /end #header -->
