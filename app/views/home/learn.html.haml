= render :partial => "header"
.container_16.searchBar.clearfix
	= render :partial => 'search_form'
	
.page
	.page-content
		.page-section-float-left
			.welcome-header Latest classes in your city
              %img.greyheader_half{:src => '../../../images/v2/greyheader_half.png'}
			.classRow
				= render :partial => 'learn_upcoming_content', :locals => {:classes_this_week => @classes}

		- if @suggestions.size > 0
			.page-section-float-left
    			.welcome-header Help make these classes happen
                  %img.greyheader_half{:src => '../../../images/v2/greyheader_half.png'}
				= render :partial => 'suggestions_for_classes', :locals => {:suggestions => @suggestions}
= render :partial => 'learn_sidebar'